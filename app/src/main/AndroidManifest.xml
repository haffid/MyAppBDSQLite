<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.haffid.myappbdsqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAppBDSQLite">
        <activity android:name=".MA_Eliminar"></activity>
        <activity android:name=".MA_Actualizar" />
        <activity android:name=".MA_Detalle" />
        <activity android:name=".MA_Mostrar" />
        <activity android:name=".MA_Buscar" />
        <activity android:name=".MA_Insertar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>